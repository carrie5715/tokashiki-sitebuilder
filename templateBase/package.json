{
  "name": "templatebase",
  "version": "1.0.0",
  "scripts": {
    "echo:id": "dotenv -e .env -- bash -lc 'echo DRIVE_FOLDER_ID=$DRIVE_FOLDER_ID'",
    "pull:dry": "dotenv -e .env -- bash -lc 'rclone sync carrie5715_googledrive: ./drive_resources --drive-root-folder-id \"$DRIVE_FOLDER_ID\" --dry-run --progress --checksum --exclude \".DS_Store\" --exclude \"Thumbs.db\"'",
    "pull":     "dotenv -e .env -- bash -lc 'rclone sync carrie5715_googledrive: ./drive_resources --drive-root-folder-id \"$DRIVE_FOLDER_ID\" --progress --checksum --create-empty-src-dirs --exclude \".DS_Store\" --exclude \"Thumbs.db\"'",
    "push:dry": "dotenv -e .env -- bash -lc 'rclone sync ./drive_resources carrie5715_googledrive: --drive-root-folder-id \"$DRIVE_FOLDER_ID\" --dry-run --progress --checksum --exclude \".DS_Store\" --exclude \"Thumbs.db\"'",
    "push":     "dotenv -e .env -- bash -lc 'rclone sync ./drive_resources carrie5715_googledrive: --drive-root-folder-id \"$DRIVE_FOLDER_ID\" --progress --checksum --exclude \".DS_Store\" --exclude \"Thumbs.db\"'",
    "download:dry": "dotenv -e .env -- bash -lc 'rclone sync carrie5715_googledrive:output ./output --drive-root-folder-id \"$PROJECT_FOLDER_ID\" --dry-run --progress --checksum --create-empty-src-dirs --exclude \".DS_Store\" --exclude \"Thumbs.db\"'",
    "download": "dotenv -e .env -- bash -lc 'rm -rf ./output && mkdir -p ./output && rclone copy carrie5715_googledrive:output ./output --drive-root-folder-id \"$PROJECT_FOLDER_ID\" --progress --checksum --create-empty-src-dirs --exclude \".DS_Store\" --exclude \"Thumbs.db\"'"
  },
  "keywords": [],
